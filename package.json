{
    "name": "garden-design-playable-ad",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build && npm run copy-assets",
        "build:vercel": "tsc && vite build && npm run copy-assets",
        "vercel-build": "npm run build:vercel",
        "copy-assets": "copyfiles -u 1 \"assets/**/*\" dist/assets",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "start": "vite preview",
        "deploy": "vercel --prod",
        "deploy:preview": "vercel"
    },
    "devDependencies": {
        "@types/three": "^0.158.3",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.0.3",
        "copyfiles": "^2.4.1",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "terser": "^5.43.1",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vercel": "^32.5.0"
    },
    "dependencies": {
        "@tweenjs/tween.js": "^21.0.3",
        "three": "^0.158.0"
    }
}